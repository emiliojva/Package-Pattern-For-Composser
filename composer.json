{
    "name": "emiliojva/package-patterns",
    "description": "Conjunto de Pacotes com abstrações ensinadas nos Cursos da SON",
    "homepage": "http://inovuerj.sr2.uerj.br",
    "authors": [
        {
            "name": "Emilio Vieira",
            "email": "emilio@sr2.uerj.br"
        }
    ],
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.0.0"
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/support": "v5.5.44"
    },
    "autoload": {
        "psr-4": {
            "Inovuerj\\": "vendor/INOVUERJ",
            "App\\": "App/"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4"
    }
}